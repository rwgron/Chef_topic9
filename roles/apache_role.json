{
   "name": "apache_role",
   "chef_type": "role",
   "json_class": "Chef::Role",
   "override_attributes": {
         "web": {
            "web_server_type":"apache",
            "config_path":"/etc/apache2/httpd.conf",
            "server_port":"80",
            "server_interface":"0.0.0.0"
  }
 },
  "description": "install apache",
  "run_list": [
     "recipe[apache_role]"
],
  "env_run_lists" : {
    "apache_env": ["recipe[apache_role]"]
  }
}
